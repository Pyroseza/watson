{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "interface-over-type-literal": false,
    "jsx-boolean-value": [
      false
    ],
    "no-increment-decrement": false,
    "no-console": [
      true,
      "debug",
      "log",
      "info"
    ],
    "prefer-array-literal": [
      true,
      {
        "allow-type-parameters": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
